  "description": "Gatsby plugin for testing context wrapping via plugin.",
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:watch": "yarn build --watch",
