    "prebuild": "rimraf dist",
    "prepublish": "yarn build",
    "release": "standard-version",
    "test": "jest",
    "test:watch": "yarn test --watch"
  },
  "homepage": "https://github.com/asyarb/gatsby-plugin-test-context",
  "repository": {
    "type": "git",
    "url": "https://github.com/asyarb/gatsby-plugin-test-context.git"
  },
